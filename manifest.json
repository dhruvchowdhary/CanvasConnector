{
  "manifest_version": 3,
  "name": "Canvas User Finder",
  "version": "1.0",
  "description": "Finds users in multiple classes on Canvas.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Canvas User Finder"
  },
  "content_scripts": [
    {
      "matches": ["*://*.bcourses.berkeley.edu/*"],
      "js": ["content.js"]
    }
  ]
}
